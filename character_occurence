# USING DICT FUNCTION

def least_char_occ(s):
    ch = {}
    for i in s:
        if i in ch:
            ch[i] = ch[i] + 1
        else:
            ch[i] = 1
    print(ch)
    result = min(ch, key = ch.get)
    print(result)
    
s = "wwesfcffffghhg"
least_char_occ(s)

